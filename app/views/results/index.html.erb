    <ul class="nav nav-list">  
      <li class="nav-header">РЕЗУЛЬТАТЫ</li>       
      <li><%= link_to "К Панели Управления", @enterprise %></li>      
    </ul>  
  </div>  
<% end  %>

<% content_for :span9 do %>
