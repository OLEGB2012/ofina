<% provide(:title, 'Новая Организация') %>

<% content_for :span3 do %>      
  <div class="well sidebar-nav">  
    <ul class="nav nav-list">  
      <li class="nav-header">Новая Организация</li>  
      <li><%= link_to "Вернуться к списку", enterprises_path %></li>               
    </ul>  
  </div>  
<% end  %>


<% content_for :span9 do %> 
  <%= simple_form_for(@enterprise,html: {class: "well form-horizontal"}) do |f| %>
    <%= render 'fields', f: f %>
    <%= f.submit "Создать", :class => "btn btn-large btn-primary" %>
  <% end %>  
<% end  %>